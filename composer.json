{
    "name": "datakrama/lapiuth",
    "description": "Laravel API Auth, change the default authentication to API-friendly",
    "type": "library",
    "require": {
        "illuminate/support": "^6.0",
        "datakrama/lapires": "^1.0",
        "laravel/passport": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^9.3@dev"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Ahmad Husen",
            "email": "husenisme@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Datakrama\\Lapiuth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Datakrama\\Lapiuth\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Datakrama\\Lapiuth\\ServiceProvider"
            ]
        }
    }
}
